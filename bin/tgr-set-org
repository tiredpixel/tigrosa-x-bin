#!/bin/bash -eu
set -o pipefail

source "$(dirname "$(readlink -f "${BASH_SOURCE[0]}")")/x-lib.sh"
#-------------------------------------------------------------------------------
ask "org.href" /org/0710c278-92a4-4af0-9ca8-c998f630112b
echo "$ASK_BUF" > "$ORG_HREF_"
reload_vars
#-------------------------------------------------------------------------------
mkdir -p "$DIR$ORG_HREF"
#-------------------------------------------------------------------------------
"$(dirname "$(readlink -f "${BASH_SOURCE[0]}")")/tgr-read" "$ORG_HREF"
#-------------------------------------------------------------------------------
