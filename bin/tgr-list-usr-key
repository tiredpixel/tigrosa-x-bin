#!/bin/bash -eu
set -o pipefail

q=${1:-}

source "$(dirname "$(readlink -f "${BASH_SOURCE[0]}")")/x-lib.sh"
#-------------------------------------------------------------------------------
ask "usr.href" "${USR_HREF:-}"
usr_href=$ASK_BUF
#-------------------------------------------------------------------------------
curla "$ENDPOINT_URL$usr_href/usr_key?$q" | jq
#-------------------------------------------------------------------------------
